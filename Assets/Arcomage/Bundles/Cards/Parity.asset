  luaSource: "function(player)\nlocal j1g = player.GamePlayerCS.gemIncRate\nlocal
    target = U.GetEnemyPlayer(player)\nlocal j2g = target.GamePlayerCS.gemIncRate\nif
    j1g < j2g then\nU.GrowthChange(player, ResourceType.Gem, j2g - j1g)\nelseif j2g
    < j1g then\nU.GrowthChange(target, ResourceType.Gem, j1g - j2g)\nend\n       
    end"
